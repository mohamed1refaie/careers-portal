[[+head]]

[[+header]]

    <section class="aboutContent">

        <div class="container">

            <div class="row mb30">
                <div class="col s12 m5">
                    <img src="[[+about_history_image]]" alt="">
                </div>
                <div class="col s12 m7">
                    <h2>[[+history_title]]</h2>
                    <p>
                        [[+about_history]]
                    </p>
                </div>
            </div>

            <div class="row">

                <div class="col s12 m7">
                    <h2>[[+mission_title]]</h2>
                    <p>
                        [[+about_mission]]
                    </p>
                    <h2>[[+quality_title]]</h2>
                    <p>
                        [[+about_quality]]
                    </p>
                </div>

                <div class="col s12 m5">
                    <img src="[[+about_mission_image]]" alt="">
                </div>
            </div>

            <div class="row" id="board">
                <h1>[[+board_members]]</h1>
                <input type="hidden" id="currPage" name="currPage" value="1" />
                <!--board member widget-->
                <div id="membersContainer">
                    

                </div>

                <div class="col s12 center mt30">
                    <a class="btn blue-btn waves-effect light-waves" id="loadMore">View More</a>
                </div>

            </div>

        </div>

    </section>

[[+extraScript]]

[[+footer]]