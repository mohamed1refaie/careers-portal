[[+head]]

[[+header]]


<section class="aboutContent">

    <div class="container">

        <div class="row mb30">

            <div class="col s12">
                <h1>[[+title]]</h1>
                [[+description]]
            </div>

            <div class="col s12 exportImages mt20">
                <div class="col s12 m6">
                    <img src="[[+main_image]]" alt="">
                </div>
                <div class="col s12 m6">
                    <div class="col s12 mb20">
                        <img src="[[+first_image]]" alt="">

                    </div>
                    <div class="col s12">
                        <img src="[[+second_image]]" alt="">

                    </div>
                </div>
                <div class="col s12 mt30 center">
                    <h2 class="">[[+exports_pdf]] </h2>
                    <a href="[[+export_url]]" class="btn blue-btn mt20" download>[[+download]]</a>
                </div>
            </div>
        </div>


    </div>

</section>


<script type="text/javascript">
    $(document).ready(function() {

        $("#loadingCont").hide();
    });
</script>
[[+footer]]